{
  "name": "@ajabhijeet21-internal/common-i18n",
  "version": "1.2.2",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./locales/*.json": {
      "require": "./src/locales/*.json",
      "import": "./src/locales/*.json"
    },
    "./package.json": "./package.json"
  },
  "author": {
    "name": "Abhijeet Jagtap",
